<section class="contact" id="contact">

    <h1 class="heading">
        Contactez <span>Nous</span>

    </h1>

    <div class="row">

        <div class="~/image">
            <img src="~/image/pharmacy.jpg" alt="picture">
        </div>

        <form action="">
            <div class="inputBox">
                <input type="text" placeholder="Nom">
                <input type="email" placeholder="E-mail">
            </div>
            <div class="inputBox">
                <input type="text" placeholder="Telephone" id="phoneNumber">
                <input type="text" placeholder="Sujet">
            </div>
            <textarea placeholder="Message" name="" id="" cols="30" rows="10"></textarea>
            <button type="submit" class="btn" onclick="phonenumber()">Envoyer</button>
        </form>

    </div>
    <script>
        function phonenumber() {
            const indiaRegex = /^\+91\d{10}$/;
            const inputText = document.getElementById("phoneNumber").value;
            if (inputText.match(indiaRegex)) {
                console.log("Valid phone number");
            } else {
                console.log("Not a valid Phone Number");
            }
        }
    </script>
</section>









